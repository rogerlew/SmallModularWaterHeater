                        <cl:DSSControl Name="DSSTankLevelLow" Title="Tank Level Low" Action="Open Fill Valve" WarningTime="{Binding DSSWarningTime}" Click="OpenFillValveHndlr" Canvas.Top="600" Canvas.Left="3"/>
