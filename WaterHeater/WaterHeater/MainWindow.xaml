                        <cl:DSSControl Name="DSSTankTempHigh" Title="Tank Temperature High" Action="Turn Heater Off" WarningTime="{Binding DSSWarningTime}" Click="TurnHeaterOffHndlr" Canvas.Top="600" Canvas.Left="3"/>
