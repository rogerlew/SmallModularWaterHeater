<UserControl x:Class="WpfControlLibrary1.ValveTestIndicator"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             DataContext="{Binding RelativeSource={RelativeSource Self}}"
             mc:Ignorable="d" 
             d:DesignHeight="46" d:DesignWidth="73">
        <Grid>
            <Rectangle Name="rect" Fill="White" Stroke="#FF666666" StrokeThickness="1" />
            <Label Name="label" Content="Open" FontSize="18" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        </Grid>
    </UserControl>

